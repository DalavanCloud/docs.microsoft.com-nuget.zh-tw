{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**",
          "archive/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.json",
          "**/*.xml"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
        "globalMetadata": {
            "bilingual_type": "hover over",
            "breadcrumb_path": "~/_breadcrumb/toc.yml",
            "ROBOTS": "INDEX,FOLLOW",
            "searchScope": ["NuGet"],
            "uhfHeaderId": "MSDocsHeader-NuGet"
            },
    "fileMetadata": {},
    "template": [],
    "dest": "nuget"
  }
}
